%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Commands
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\newcommand{\newDimension}[2]{%
\newcommand{#1}[1][]{%
\def\temp{##1} \ifx\temp\empty%
\mathsf{#2}
\else
\mathsf{#2}^{##1}
\fi
}
}

\newcommand{\newQuantity}[2]{%
\newcommand{#1}[1][]{%
\def\temp{##1} \ifx\temp\empty%
{#2}
\else
{#2}_{##1}
\fi
}
}

\newcommand{\newLabeledQuantity}[3]{%
\newcommand{#1}[1][]{%
\def\temp{##1} \ifx\temp\empty%
{#2}_{#3}
\else
{#2}_{#3,##1}
\fi
}
}

\newcommand{\newComponentQuantity}[4]{%
\newcommand{#1}[1]{%
\def\temp{##1}
\def\tempone{1}
\def\temptwo{2}
\def\tempthree{3}
\ifx\temp\tempone%
#2
\else
\ifx\temp\temptwo%
#3
\else
\ifx\temp\tempthree%
#4
\else
{#2}_{##1}
\fi
\fi
\fi
}
}

\newcommand{\newNumberedComponentQuantity}[2]{%
\newcommand{#1}[1]{%
{#2}_{##1}
}
}

\newcommand{\newLabeledComponentQuantity}[5]{%
\newcommand{#1}[1]{%
\def\temp{##1}
\def\tempone{1}
\def\temptwo{2}
\def\tempthree{3}
\ifx\temp\tempone%
{#2}_{#3}
\else
\ifx\temp\temptwo%
{#2}_{#4}
\else
\ifx\temp\tempthree%
{#2}_{#5}
\else
{#2}_{##1}
\fi
\fi
\fi
}
}

\newcommand{\newExtensiveQuantity}[2]{%
\newQuantity{#1}{\uppercase{#2}}
}

\newcommand{\newSpecificQuantity}[2]{%
\newQuantity{#1}{\lowercase{#2}}
}

\newcommand{\newMolarQuantity}[2]{%
\newQuantity{#1}{\hat{\lowercase{#2}}}
}

